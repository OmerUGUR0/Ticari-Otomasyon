@model MVC_Ticari_Otomasyon.Models.Classes.Department
@{
    ViewBag.Title = "DepartmentAdd";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<br />
<h2>Yeni Departman Ekle</h2>

<form class="form-group" method="post">
    @Html.Label("Departman İsmi :")
    @Html.TextBoxFor(x => x.DepartmentName, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

